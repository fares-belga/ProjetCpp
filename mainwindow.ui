     <property name="styleSheet">
      <string notr="true">/* Styles de la ComboBox avec des couleurs différentes */
QComboBox {
    background-color: #dfe6e9; /* Gris clair */
    border: 1px solid #bdc3c7; /* Bordure grise légèrement plus foncée */
    border-radius: 10px;
    padding: 5px;
    width: 70%;
    color: #2C3E50; /* Texte en bleu foncé */
    font-size: inherit;
    font-family: Arial, Helvetica, sans-serif;
    font-weight: normal;
}

QComboBox:hover {
    color: #000000; /* Texte en noir au survol */
    background-color: #ecf0f1; /* Gris clair au survol */
}

QComboBox:on {
    color: #000000; /* Texte en noir lorsqu'il est ouvert */
    background-color: #ffffff; /* Fond blanc lorsqu'il est ouvert */
}

QComboBox::down-arrow {
    image: url(down_arrow.png); /* Remplacez par le chemin de votre image de flèche vers le bas */
}

QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 20px;
    border-left-width: 1px;
    border-left-color: #bdc3c7; /* Bordure grise légèrement plus foncée */
    border-left-style: solid; /* Style de la bordure */
    border-top-right-radius: 10px; /* Bordure arrondie en haut à droite */
    border-bottom-right-radius: 10px; /* Bordure arrondie en bas à droite */
}

QComboBox::down-arrow:on {
    top: 1px;
    left: 1px;
}

</string>
