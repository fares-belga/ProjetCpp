       <property name="styleSheet">
        <string notr="true">/* Styles du QLineEdit avec des couleurs différentes */
QLineEdit {
    background-color: #dfe6e9; /* Gris clair */
    box-shadow: 2px 0.5px 5px 1px inset #000000;
    border-radius: 10px;
    width: 70%;
    padding-right: 10px;
    padding-left: 10px;
    text-align: left;
    color: #2C3E50; /* Bleu foncé */
    font-size: inherit;
    font-family: Arial, Helvetica, sans-serif;
    font-weight: normal;
    font-variant: normal;
    border-color: #bdc3c7; /* Gris légèrement plus foncé */
}

QLineEdit::hover {
    color: #000000; /* Noir */
    background-color: #ecf0f1; /* Gris clair au survol */
}

QLineEdit::focus {
    color: #000000; /* Noir */
    background-color: #ffffff; /* Blanc */
}
</string>
